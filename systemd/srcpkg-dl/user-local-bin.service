After=network.target

[Service]
# edit this variable (WorkingDirectory) value if necessary (full PATH) - '~'/'$HOME' may be '/root' (/root/.local/bin)
#WorkingDirectory=~/.local/bin
#ExecStart=./srcpkg-dl
